
class Main {

	function void main() {
		do Sys.args();
		do Sys.wait(10);
		do Sys.deInit();
		do Unit.setLevel(2);
		do Unit.log(0, "Sys", Memory.checkEmpty());	
		do Unit.log(0, "String", UnitString.isOk(0));	
		do Unit.log(0, "File", UnitFile.isOk(0));	
		do Unit.log(0, "Xml", UnitXml.isOk(0));	
		do Unit.log(0, "JackCompiler", UnitJackCompiler.isOk(0));	
		do Unit.log(0, "Bytes", UnitBytes.isOk(0));	
	}
}
